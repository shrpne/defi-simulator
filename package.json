{
  "name": "defi-simulator",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && npm run build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "analyze": "npx vite-bundle-analyzer",
    "swagger:coingecko": "swagger-typescript-api generate -p 'https://docs.coingecko.com/openapi/65bf9cabb0951b0072e2cade' --no-client --extract-request-params --extract-response-body --type-prefix CoinGecko -o ./src/lib/api -n ${npm_config_file_name:=coingecko-models.d.ts} --custom-config scripts/swagger-typescript-api/coingecko.js",
    "swagger:pendle": "swagger-typescript-api generate -p 'https://api-v2.pendle.finance/core/docs-json' --no-client --extract-request-params --extract-response-body --type-prefix Pendle -o ./src/lib/api -n ${npm_config_file_name:=pendle-models.d.ts} --custom-config scripts/swagger-typescript-api/pendle.js"
  },
  "dependencies": {
    "@lifi/sdk": "^3.6.11",
    "@reown/appkit": "^1.7.3",
    "@reown/appkit-adapter-wagmi": "^1.7.3",
    "@tanstack/vue-query": "^5.75.1",
    "@vueuse/core": "^13.2.0",
    "@wagmi/vue": "^0.1.18",
    "axios": "^1.9.0",
    "camelcase": "^8.0.0",
    "camelcase-keys": "^9.1.3",
    "daisyui": "^5.0.35",
    "decamelize-keys": "^2.0.1",
    "entity-decode": "^3.0.0",
    "es-toolkit": "^1.37.1",
    "moralis": "^2.27.2",
    "pretty-num": "^0.6.2",
    "viem": "^2.28.3",
    "vue": "^3.5.13",
    "vue-imask": "^7.6.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.5",
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.15.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.26.0",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "swagger-typescript-api": "^13.1.3",
    "tailwindcss": "^4.1.5",
    "type-fest": "^4.40.1",
    "typescript": "~5.8.3",
    "vite": "^6.3.4",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10"
  }
}
