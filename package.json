{
  "name": "defi-simulator",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "swagger:coingecko": "swagger-typescript-api -p 'https://docs.coingecko.com/openapi/65bf9cabb0951b0072e2cade' --no-client --extract-request-params --extract-response-body --type-prefix CoinGecko -o ./src/lib/api -n ${npm_config_file_name:=coingecko-models.d.ts} --custom-config scripts/swagger-typescript-api/coingecko.js",
    "swagger:pendle": "swagger-typescript-api -p 'https://api-v2.pendle.finance/core/docs-json' --no-client --extract-request-params --extract-response-body --type-prefix Pendle -o ./src/lib/api -n ${npm_config_file_name:=pendle-models.d.ts} --custom-config scripts/swagger-typescript-api/pendle.js"
  },
  "dependencies": {
    "@lifi/sdk": "^3.6.2",
    "@primevue/themes": "^4.3.2",
    "@web3-onboard/injected-wallets": "^2.11.3",
    "@web3-onboard/vue": "^2.10.0",
    "axios": "^1.8.4",
    "camelcase": "^8.0.0",
    "camelcase-keys": "^9.1.3",
    "decamelize-keys": "^2.0.1",
    "entity-decode": "^3.0.0",
    "moralis": "^2.27.2",
    "pretty-num": "^0.6.0",
    "primevue": "^4.3.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.15",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.11",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "swagger-typescript-api": "^13.0.24",
    "tailwindcss": "^4.0.15",
    "type-fest": "^4.37.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
