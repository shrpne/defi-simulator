{
  "name": "eth-gas-simulator",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "swagger:pendle": "swagger-typescript-api -p 'https://api-v2.pendle.finance/core/docs-json' --no-client --extract-request-params --extract-response-body --type-prefix Pendle -o ./src/lib/api -n ${npm_config_file_name:=pendle-models.d.ts} --custom-config scripts/swagger-typescript-api/index.js"
  },
  "dependencies": {
    "@primevue/themes": "^4.2.5",
    "@web3-onboard/injected-wallets": "^2.11.3",
    "@web3-onboard/vue": "^2.10.0",
    "axios": "^1.7.9",
    "entity-decode": "^3.0.0",
    "moralis": "^2.27.2",
    "pretty-num": "^0.6.0",
    "primevue": "^4.2.5",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.3",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.19.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "swagger-typescript-api": "^13.0.23",
    "tailwindcss": "^4.0.3",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vue-tsc": "^2.2.0"
  }
}
